= form_for @gift do |f|
  - if @gift.errors.any?
    #error_explanation
      %h2= "#{pluralize(@gift.errors.count, "error")} prohibited this gift from being saved:"
      %ul
        - @gift.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :to_user
    = f.number_field :to_user
  .field
    = f.label :from_user
    = f.number_field :from_user
  .field
    = f.label :typeObj
    = f.text_field :typeObj
  .field
    = f.label :obj_id
    = f.number_field :obj_id
  .actions
    = f.submit 'Save'
