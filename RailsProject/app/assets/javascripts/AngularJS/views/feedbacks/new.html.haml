.new_feedback
	%h2 Feedbacks
	%hr
	.row{ "ng-show" => "!send && !isSending" }
		.large-3.small-3.columns.inline
			%label{:for => "feedback_email"} Email
		.small-9.columns
			%input#feedback_email.radius{ 'ng-model' => "feedback.email", :type => "text"}
		.large-3.small-3.columns.inline
			%label{:for => "feedback_type_object"} Type object
		.small-9.columns
			%select#feedback_type_object{ 'ng-model' => "feedback.type_object", 'ng-change' => 'verifType()'}
				%option{ "ng-repeat" => "data in options", value: "{{ data.value }}" } {{ data.label }}
			{{ feedback.type_object }}
		.large-3.small-3.columns.inline
			%label{:for => "feedback_object"} Object
		.small-9.columns
			%input#feedback_object.radius{ 'ng-model' => "feedback.object", :type => "text"}
		.large-3.small-3.columns.inline
			%label{:for => "feedback_text"} Text
		.small-9.columns
			%textarea#feedback_text.radius{ 'ng-model' => "feedback.text"}
		.small-12.columns
			%span.button.success{ "ng-click" => "sendFeeback()"} Save
	%loading{ "ng-show" => "isSending" }
	.feedbackSend.text-center{ "ng-show" => "send" } 
